-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

javac.source: 1.8
javac.target: 1.8

Bundle-SymbolicName: com.ibm.websphere.org.eclipse.microprofile.reactive.streams.operators.1.0; singleton:=true

Import-Package: \
  org.reactivestreams; version="[1.0.2,1.1)",\
  javax.enterprise.util; version="[1.1,3)",\
  *

Export-Package: \
  org.eclipse.microprofile.reactive.streams;version=1.0, \
  org.eclipse.microprofile.reactive.streams.spi;version=1.0, \
  org.reactivestreams;version=1.0.2, \

Include-Resource: \

Require-Capability: osgi.ee;filter:="(&(osgi.ee=JavaSE)(version=1.8))"

WS-TraceGroup: FAULTTOLERANCE

-buildpath: \
	com.ibm.websphere.javaee.cdi.1.2;version=latest, \
	com.ibm.websphere.javaee.interceptor.1.2;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning;version=latest,  \
        com.ibm.websphere.org.reactivestreams.reactive-streams.1.0.2;version=latest

instrument.disabled: true

publish.wlp.jar.suffix: dev/api/stable

