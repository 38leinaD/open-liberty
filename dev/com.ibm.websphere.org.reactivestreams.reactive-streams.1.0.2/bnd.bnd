-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0.2

javac.source: 1.8
javac.target: 1.8

Bundle-SymbolicName: com.ibm.websphere.org.reactivestreams.reactive-streams.1.0.2; singleton:=true
Require-Capability: osgi.ee;filter:="(&(osgi.ee=JavaSE)(version=1.8))"

Import-Package: \

Export-Package: \
  org.reactivestreams; version=1.0.2, \
  org.reactivestreams.tck; version=1.0.2

Include-Resource: \
  @${repo;org.reactivestreams:reactive-streams;1.0.2;EXACT},\
  @${repo;org.reactivestreams:reactive-streams-tck;1.0.2;EXACT}

WS-TraceGroup: REACTIVESTREAMS

instrument.disabled: true

-buildpath: \

publish.wlp.jar.suffix: dev/api/spec

